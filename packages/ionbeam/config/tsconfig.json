{
  "$schema": "https://www.schemastore.org/tsconfig",
  "_version": "2.0.0",
  "compilerOptions": {
    "target": "ES2023",
    "module": "es2022",
    "lib": [
      "ES2023"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "moduleResolution": "bundler",
    "jsx": "react-jsx"
  }
}
